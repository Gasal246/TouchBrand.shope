<p>The following addresses will be used on the checkout page
    by default.</p>

<div class="row">
    <div class="col-lg-6">
        <div class="card card-dashboard">
            <div class="card-body">
                <h3 class="card-title">Primary Address</h3>
                <!-- End .card-title -->
                <% if(address){ %>
                    <p>
                        <%= address.Firstaddress.Cname %>,<br>
                            <%= address.Firstaddress.City %>
                                ,
                                <%= address.Firstaddress.Place
                                    %><br>
                                    <%= address.Firstaddress.Landmark
                                        %><br>
                                        <%= address.Firstaddress.Country
                                            %><br>
                                            <%= address.Firstaddress.Pincode
                                                %><br>
                                                phone : <%=
                                                    cookies.phone
                                                    %> <br>
                                                    <% } else
                                                        { %>
                                                        You
                                                        have
                                                        not
                                                        set
                                                        up
                                                        this
                                                        type
                                                        of
                                                        address
                                                        yet.
                                                        <% }
                                                            %>
                                                            <a href="#address-modal-1"
                                                                data-toggle="modal">Change
                                                                <i
                                                                    class="icon-edit"></i></a>
                    </p>
            </div><!-- End .card-body -->
        </div><!-- End .card-dashboard -->
    </div><!-- End .col-lg-6 -->

    <!-- Primary Address form -->
    <div class="modal fade" id="address-modal-1" tabindex="-1"
    role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered"
        role="document">
        <div class="modal-content">
            <div class="modal-body">
                <button type="button" class="close"
                    data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true"><i
                            class="icon-close"></i></span>
                </button>

                <div class="form-box">
                    <div class="form-tab">
                        <ul class="nav nav-pills nav-fill"
                            role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active"
                                    id="signin-tab"
                                    data-toggle="tab"
                                    href="#signin" role="tab"
                                    aria-controls="signin"
                                    aria-selected="true">Primary
                                    Address</a>
                            </li>
                        </ul>
                        <div class="tab-content"
                            id="tab-content-5">
                            <div class="tab-pane fade show active"
                                id="signin" role="tabpanel"
                                aria-labelledby="signin-tab">
                                <form action="/primaryaddress"
                                    method="post">
                                    <div class="mb-3">
                                        <label
                                            class="form-label">Costumer Name</label>
                                        <input type="text"
                                            class="form-control"
                                            name="cname"
                                            value="<%= address?address.Firstaddress.Cname:null %>">
                                    </div>
                                    <div class="mb-3">
                                        <label
                                            class="form-label">Country</label>
                                        <input type="text"
                                            class="form-control"
                                            name="country"
                                            value="<%= address?address.Firstaddress.Country:null %>">
                                    </div>
                                    <div class="mb-3">
                                        <label
                                            class="form-label">city</label>
                                        <input type="text"
                                            class="form-control"
                                            name="city"
                                            value="<%= address?address.Firstaddress.City:null %>">
                                    </div>
                                    <div class="mb-3">
                                        <label
                                            class="form-label">place</label>
                                        <input type="text"
                                            class="form-control"
                                            name="place"
                                            value="<%= address?address.Firstaddress.Place:null %>">
                                    </div>
                                    <div class="mb-3">
                                        <label
                                            class="form-label">landmark</label>
                                        <input type="text"
                                            class="form-control"
                                            name="landmark"
                                            value="<%= address?address.Firstaddress.Landmark:null %>">
                                    </div>
                                    <div class="mb-3">
                                        <label
                                            class="form-label">pincode</label>
                                        <input type="text"
                                            class="form-control"
                                            name="pincode"
                                            value="<%= address?address.Firstaddress.Pincode:null %>">
                                    </div>
                                    <button type="submit"
                                        class="btn btn-primary">Save
                                        it</button>
                                </form>

                            </div>

                        </div><!-- End .tab-content -->
                    </div><!-- End .form-tab -->
                </div><!-- End .form-box -->
            </div><!-- End .modal-body -->
        </div><!-- End .modal-content -->
    </div><!-- End .modal-dialog -->
</div><!-- End .modal -->


    <!-- Secondary Address form -->
    <div class="modal fade" id="address-modal-2"
        tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered"
            role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <button type="button" class="close"
                        data-dismiss="modal"
                        aria-label="Close">
                        <span aria-hidden="true"><i
                                class="icon-close"></i></span>
                    </button>

                    <div class="form-box">
                        <div class="form-tab">
                            <ul class="nav nav-pills nav-fill"
                                role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active"
                                        id="signin-tab"
                                        data-toggle="tab"
                                        href="#signin"
                                        role="tab"
                                        aria-controls="signin"
                                        aria-selected="true">Secondary
                                        Address</a>
                                </li>
                            </ul>
                            <div class="tab-content"
                                id="tab-content-5">
                                <div class="tab-pane fade show active"
                                    id="signin"
                                    role="tabpanel"
                                    aria-labelledby="signin-tab">
                                    <form
                                        action="/secondaryaddress"
                                        method="post">
                                        <div class="mb-3">
                                            <label
                                                class="form-label">Costumer name</label>
                                            <input
                                                type="text"
                                                class="form-control"
                                                name="cname"
                                                value="<%= address?address.Secondaddress.Cname:null %>">
                                        </div>
                                        <div class="mb-3">
                                            <label
                                                class="form-label">Country</label>
                                            <input
                                                type="text"
                                                class="form-control"
                                                name="country"
                                                value="<%= address?address.Secondaddress.Country:null %>">
                                        </div>
                                        <div class="mb-3">
                                            <label
                                                class="form-label">city</label>
                                            <input
                                                type="text"
                                                class="form-control"
                                                name="city"
                                                value="<%= address?address.Secondaddress.City:null %>">
                                        </div>
                                        <div class="mb-3">
                                            <label
                                                class="form-label">place</label>
                                            <input
                                                type="text"
                                                class="form-control"
                                                name="place"
                                                value="<%= address?address.Secondaddress.Place:null %>">
                                        </div>
                                        <div class="mb-3">
                                            <label
                                                class="form-label">landmark</label>
                                            <input
                                                type="text"
                                                class="form-control"
                                                name="landmark"
                                                value="<%= address?address.Secondaddress.Landmark:null %>">
                                        </div>
                                        <div class="mb-3">
                                            <label
                                                class="form-label">pincode</label>
                                            <input
                                                type="text"
                                                class="form-control"
                                                name="pincode"
                                                value="<%= address?address.Secondaddress.Pincode:null %>">
                                        </div>
                                        <button
                                            type="submit"
                                            class="btn btn-primary">Save
                                            it</button>
                                    </form>

                                </div>

                            </div><!-- End .tab-content -->
                        </div><!-- End .form-tab -->
                    </div><!-- End .form-box -->
                </div><!-- End .modal-body -->
            </div><!-- End .modal-content -->
        </div><!-- End .modal-dialog -->
    </div><!-- End .modal -->

    <div class="col-lg-6">
        <div class="card card-dashboard">
            <div class="card-body">
                <h3 class="card-title">Secondary Address
                </h3><!-- End .card-title -->
                <% if(address){ %>
                    <p>
                        <%= address.Secondaddress.Cname %>,<br>
                        <%= address.Secondaddress.City %>,
                            <%= address.Secondaddress.Place
                                %><br>
                                <%= address.Secondaddress.Landmark
                                    %><br>
                                    <%= address.Secondaddress.Country
                                        %><br>
                                        <%= address.Secondaddress.Pincode
                                            %><br>
                                            <% } else { %>
                                                You have not
                                                set up this
                                                type of
                                                address yet.
                                                <% } %>
                                                    <a href="#address-modal-2"
                                                        data-toggle="modal">Change
                                                        <i
                                                            class="icon-edit"></i></a>
                    </p>
            </div><!-- End .card-body -->
        </div><!-- End .card-dashboard -->
    </div><!-- End .col-lg-6 -->
</div><!-- End .row -->